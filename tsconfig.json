{
	"compilerOptions": {
		"allowUnreachableCode": false,
		"alwaysStrict": true,
		"baseUrl": ".",
		"checkJs": true,
		"declaration": true,
		"forceConsistentCasingInFileNames": true,
		"module": "node16",
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"outDir": "dist",
		"rootDir": ".",
		"skipLibCheck": true,
		"strict": true,
		"target": "es2022"
	}
}
